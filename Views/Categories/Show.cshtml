@model OpenDiscussion.Models.Category

<p>@Model.CategoryName</p>

<br/>
<br/>

@foreach (OpenDiscussion.Models.Topic top in @Model.Topics ?? new List<Topic>())
{
    <div class="card">
        <a class="btn btn-success" href="/Topics/Show/@top.Id">
        @Html.Partial("TopicInfo", top)    
        </a>
    </div>

    <br/>
    <br/>
}

<br />
<hr />
<br />